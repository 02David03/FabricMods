{
  "schemaVersion": 1,
  "id": "ninjaphenix-container-library",
  "version": "${mod_version}",
  "name": "NinjaPhenix's Container Library",
  "description": "Provides an easy way to open chest like containers.",
  "authors": [
    "NinjaPhenix"
  ],
  "contact": {
    "sources": "https://github.com/NinjaPhenix/FabricMods/tree/${minecraft_version}",
    "issues": "https://github.com/NinjaPhenix/FabricMods/issues"
  },
  "license": "MIT-0",
  "environment": "*",
  "mixins": [
    "ninjaphenix-container-library.common.mixins.json"
  ],
  "entrypoints": {
    "main": [
      "ninjaphenix.container_library.impl.common.ContainerLibrary::INSTANCE"
    ],
    "client": [
      "ninjaphenix.container_library.impl.client.ContainerLibraryClient::INSTANCE"
    ]
  },
  "accessWidener": "ninjaphenix-container-library.accesswidener",
  "depends": {
    "fabric": ">=${dep_fabric_version}"
  },
  "custom": {
    "modmenu:api": true
  }
}
