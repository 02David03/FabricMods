{
  "schemaVersion": 1,
  "id": "userdefinedadditions",
  "version": "${mod_version}",
  "name": "User Defined Additions",
  "description": "Allows the creation of additional content through json.",
  "authors": [
    "NinjaPhenix"
  ],
  "contact": {
    "sources": "https://github.com/NinjaPhenix/FabricMods/tree/${minecraft_version}",
    "issues": "https://github.com/NinjaPhenix/FabricMods/issues"
  },
  "license": "MIT",
  "icon": "assets/userdefinedadditions/logo.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "ninjaphenix.userdefinedadditions.ClientMod"
    ],
    "main": [
      "ninjaphenix.userdefinedadditions.CommonMod"
    ]
  },
  "mixins": [
    {
      "config": "userdefinedadditions.mixins.common.json",
      "environment": "*"
    }
  ],
  "depends": {
    "fabricloader": ">=${dep_loader_version}",
    "fabric": ">=${dep_fabric_version}",
    "minecraft": ">=${dep_minecraft_version}"
  }
}